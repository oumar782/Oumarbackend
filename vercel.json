{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(contact|projet|health|ping)",
      "dest": "/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/",
      "dest": "/server.js",
      "methods": ["GET"]
    }
  ]
}